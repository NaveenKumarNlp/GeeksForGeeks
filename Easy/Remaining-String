class Solution {
    public String printString(String s, char ch, int count) {
        // code here
        int index=-1;
        for(int i=0;i<s.length();i++){
            if(ch==s.charAt(i)){
                index=i;
                count--;
            }
            if(count==0){
                return s.substring(i+1,s.length());
            }
        }
        return "";
    }
}
