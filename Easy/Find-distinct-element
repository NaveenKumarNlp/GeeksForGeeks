static int distinct(int arr[], int n) {
    // code here
    HashSet<Integer> set=new HashSet<>();
    for(int i:arr){
        set.add(i);
    }
    return set.size();
}

//{ Driver Code Starts.

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while (t-- > 0) {
            int n = sc.nextInt();
            int[] a = new int[n];
            for (int i = 0; i < n; i++) a[i] = sc.nextInt();

            System.out.println(distinct(a, n));
        
System.out.println("~");
}
    }
}
