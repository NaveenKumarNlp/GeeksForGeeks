// User function Template for Java

class Solution {
    static String findLargest(int n, int s) {
        // code here
        StringBuilder sb=new StringBuilder();
        if(n>1 && s==0){sb.append(-1); return sb.toString();}
        for(int i=0;i<n;i++){
            int t=9;
            while(t>s && t>=0){
                t--;
            }
            sb.append(t);
            s-=t;
        } if(s!=0){sb=new StringBuilder();sb.append(-1);}
        return sb.toString();
    }
}
