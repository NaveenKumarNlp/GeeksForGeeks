class Solution {
    public int count(String s) {
        // code here
        int []temp = new int[26];
        for(int i=0;i<s.length();i++){
            temp[s.charAt(i)-'a']++;
        }
        int ans=0;
        for(int i=0;i<26;i++){
            if(temp[i]!=0 && temp[i]%2==0){
                ans+=1;
            }
        }
        return ans;
    }
}
