#User function Template for python3
class Solution:
    def maxArea(self, arr):
        #code
        max=0
        for i in arr :
            if max<i:
                max=i;
        return max*max



#{ 
 # Driver Code Starts.
#Initial Template for Python 3

if __name__ == "__main__":
    t = int(input())
    while t > 0:
        arr = list(map(int, input().split()))
        ob = Solution()
        res = ob.maxArea(arr)
        print(res)
        print("~")
        t -= 1


# } Driver Code Ends
